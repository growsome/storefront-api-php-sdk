{
    "name": "scayle/storefront-api",
    "description": "SCAYLE Storefront API SDK",
    "type": "library",
    "require": {
      "ext-json": "*",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Scayle\\StorefrontApi\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scayle\\StorefrontApi\\": [
                "tests/"
            ]
        }
    }
}
